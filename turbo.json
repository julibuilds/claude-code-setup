{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"inputs": ["$TURBO_DEFAULT$", "biome.json"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"ci": {
			"dependsOn": ["^ci"],
			"inputs": ["$TURBO_DEFAULT$", "biome.json"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
